    <licenseUrl>https://github.com/Divergic/Divergic.Logging.Sentry/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/Divergic/Divergic.Logging.Sentry</projectUrl>
    <tags>Sentry;Logging;Autofac;NodaTime</tags>

    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Divergic.Logging.Sentry" version="$(Build.BuildNumber)" />
        <dependency id="Divergic.Logging.Sentry.Autofac" version="$(Build.BuildNumber)" />
        <dependency id="Divergic.Logging.Sentry.NodaTime" version="$(Build.BuildNumber)" />
      </group>
    </dependencies>
