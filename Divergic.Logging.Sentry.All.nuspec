<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Divergic.Logging.Sentry.All</id>
    <version>$(Build.BuildNumber)</version>

    <title>NodaTime serialization for ILogger with Sentry.io</title>
