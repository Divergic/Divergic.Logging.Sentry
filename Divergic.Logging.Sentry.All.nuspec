<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Divergic.Logging.Sentry.All</id>
    <version>$(Build.BuildNumber)</version>

    <Title>NodaTime serialization for ILogger with Sentry.io</Title>
    <Authors>Rory Primrose</Authors>
    <Company>Divergic Ptd Ltd</Company>
    <Description>This library contains support for NodaTime serialization when logging exceptions to Sentry.io.</Description>
    <Copyright>Copyright Â© Divergic Ptd Ltd 2018</Copyright>
    <PackageLicenseUrl>https://github.com/Divergic/Divergic.Logging.Sentry/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Divergic/Divergic.Logging.Sentry</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Divergic/Divergic.Logging.Sentry</RepositoryUrl>
    <PackageTags>Sentry;Logging;Autofac;NodaTime</PackageTags>

    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Divergic.Logging.Sentry" version="$(Build.BuildNumber)" />
        <dependency id="Divergic.Logging.Sentry.Autofac" version="$(Build.BuildNumber)" />
        <dependency id="Divergic.Logging.Sentry.NodaTime" version="$(Build.BuildNumber)" />
      </group>
    </dependencies>
  </metadata>
</package>